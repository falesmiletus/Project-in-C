{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Debug (gdb Attach)",
      "type": "cppdbg",
      "request": "attach",
      "program": "enter program name, for example: ${workspaceFolder}/a.out",
      "processId": "${command:pickProcess}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/programer/Документы/Project C/Project-in-C/Project1",
      "program": "/home/programer/Документы/Project C/Project-in-C/Project1/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}